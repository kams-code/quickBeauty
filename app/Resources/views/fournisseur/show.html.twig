{% extends 'base.html.twig' %}

{% block body %}
    <h1>Fournisseur</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fournisseur.id }}</td>
            </tr>
            <tr>
                <th>Nomfour</th>
                <td>{{ fournisseur.nomFour }}</td>
            </tr>
            <tr>
                <th>Prenomfour</th>
                <td>{{ fournisseur.prenomFour }}</td>
            </tr>
            <tr>
                <th>Adressefour</th>
                <td>{{ fournisseur.adresseFour }}</td>
            </tr>
            <tr>
                <th>Telfour</th>
                <td>{{ fournisseur.telFour }}</td>
            </tr>
            <tr>
                <th>Villefour</th>
                <td>{{ fournisseur.villeFour }}</td>
            </tr>
            <tr>
                <th>Emailfour</th>
                <td>{{ fournisseur.emailFour }}</td>
            </tr>
            <tr>
                <th>Datefour</th>
                <td>{% if fournisseur.dateFour %}{{ fournisseur.dateFour|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('fournisseur_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('fournisseur_edit', { 'id': fournisseur.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
